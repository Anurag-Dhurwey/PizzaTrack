<%if(orderData.length) {%>
    <% orderData.forEach((order)=>{ %>
        
      <tr>
          <td class="border px-4 py-2">
              <a class="link" href="/"><%= order._id%></a>
          </td>
          <td class="border px-4 py-2">
            <%= order.address%>
          </td>
          <td class="border px-4 py-2">
            <%= order.createdAt%>
          </td>
          <td class="border px-4 py-2">
            Active
          </td>
      </tr>
      
      <% }) %>
      

   <%}else{%>
       <tr class="border">
           <td class=" px-4 py-2">
               <a class="link" >No orders found</a>
           </td>
           
           
       </tr>
<%}%>