<% model.forEach((item)=>{%>
     
    <div>
        <img class="h-40 mb-4 mx-auto" src="img/pizza.png">
        <div class="">
            <h2><%= item.name%></h2>
            <span><%= item.size%></span>
            <div class="flex justify-evenly items-center mt-4">
                <span class="font-bold text-lg"> &#8377; <%= item.price%></span>
                <button class="Add-to-cart rounded-full px-4">
                     <span class="pr-3">+</span>
                     <span>Add</span>
                </button>
            </div>
        </div>
       </div>

  <% })%>